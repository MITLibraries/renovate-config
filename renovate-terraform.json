{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>MITLibraries/renovate-config"
  ],
  "baseBranches": [
    "dev"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "hashicorp/aws"
      ],
      "allowedVersions": "<6.0"
    },
    {
      "matchPackageNames": [
        "hashicorp/terraform"
      ],
      "allowedVersions": "<=1.12.0"
    }
  ]
}
